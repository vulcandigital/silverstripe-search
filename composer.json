{
  "name": "vulcandigital/silverstripe-search",
  "description": "A full text search replacement concept for SilverStripe that enables the use of tanks",
  "type": "silverstripe-vendormodule",
  "homepage": "https://github.com/vulcandigital/silverstripe-search",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Reece Alexander",
      "email": "reece@vulcandigital.co.nz"
    }
  ],
  "require": {
    "silverstripe/framework": "^4.0"
  },
  "require-dev": {
    "phpunit/PHPUnit": "^5.7",
    "squizlabs/php_codesniffer": "3.*"
  },
  "autoload": {
    "psr-4": {
      "Vulcan\\Search\\": "src/"
    }
  },
  "minimum-stability": "dev",
  "extra": {
    "expose": [
      "js"
    ]
  }
}
